{
  "name": "vk-mini-apps-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "VK Mini App",
  "scripts": {
    "dev": "vite",
    "vk tunnel": "vk-tunnel",
    "build": "tsc && vite build",
    "odr": "tsc && vite --config vite.config.odr.mts build",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{tsx,ts,css}'",
    "deploy": "tsc && vite build && vk-miniapps-deploy"
  },
  "dependencies": {
    "@vkontakte/icons": "2.165.0",
    "@vkontakte/vk-bridge": "2.15.4",
    "@vkontakte/vk-bridge-react": "1.0.1",
    "@vkontakte/vk-mini-apps-router": "1.7.3",
    "@vkontakte/vkjs": "2.0.1",
    "@vkontakte/vkui": "7.1.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "5.0.0",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@eslint/js": "9.20.0",
    "@types/eslint": "9.6.1",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "@vitejs/plugin-react": "4.3.4",
    "@vkontakte/vk-miniapps-deploy": "0.1.9",
    "@vkontakte/vk-tunnel": "0.2.4",
    "autoprefixer": "10.4.20",
    "eruda": "3.4.1",
    "eruda-dom": "2.0.0",
    "eslint": "9.20.1",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "postcss": "8.5.2",
    "postcss-preset-env": "10.1.4",
    "prettier": "3.5.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.0",
    "vite": "6.1.0",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-singlefile": "2.1.0",
    "vite-plugin-zip-pack": "1.2.4",
    "vite-tsconfig-paths": "5.1.4"
  }
}
